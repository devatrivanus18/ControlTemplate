<?xml version="1.0" encoding="utf-8" ?>
<StackLayout xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Name="conTabel"
             x:Class="ControlTemplate.Controls.TabelControl.TabelControl" BindingContext="{x:Reference conTabel}">
    <CollectionView ItemsSource="{Binding Data}"
                       Margin="0,10,0,0">
        <CollectionView.Header>
            <ContentView x:Name="ContentHeader" />
        </CollectionView.Header>
        <CollectionView.ItemTemplate>
            <DataTemplate>
                <ContentView x:Name="KontenTabel" Content="{Binding ContentTabel}" />
            </DataTemplate>
        </CollectionView.ItemTemplate>
    </CollectionView>
</StackLayout>
